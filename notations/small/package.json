{
  "name": "@antimatter-dimensions/notations-small",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "description": "Notations used in Antimatter Dimensions, built for break_eternity.js. This set of notations is specifically built for numbers less than 1e9e15",
  "scripts": {
    "build": "npm run build:ad && npm run build:community",
    "build:ad": "rolldown --config rolldown.config.ts",
    "build:community": "rolldown --config rolldown.community.ts",
    "lint": "oxlint . --fix && eslint . --fix",
    "test": "jest"
  },
  "main": "dist/ad-notations.umd.js",
  "module": "dist/ad-notations.esm.js",
  "unpkg": "dist/ad-notations.min.js",
  "jsdelivr": "dist/ad-notations.min.js",
  "keywords": [
    "notations",
    "numbers"
  ],
  "license": "MIT",
  "files": [
    "dist",
    "docs",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/HexaVault/BEnotations-Pre.git"
  },
  "bugs": {
    "url": "https://github.com/HexaVault/BEnotations-Pre/issues"
  },
  "homepage": "https://github.com/HexaVault/BEnotations-Pre",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.5.14",
    "rolldown": "^1.0.0-beta.9",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "rollup-plugin-swc3": "^0.12.1",
    "ts-jest": "^29.3.4"
  },
  "dependencies": {
    "tslib": "^2.0.0",
    "break_eternity.js": "^2.1.1"
  }
}
